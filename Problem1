#include <iostream>

int main()
{
    int threem;
    int fivem;
    int sum;
    int i;
    int n = 1000;
    for(i=1 ; i<n ; i++)
    {
        if(i%3==0 && i%5!=0)
        {
            sum +=i;
        }
        if(i%5==0 && i%3!=0)
        {
            sum +=i;
        }
        if(i%5==0 && i%3==0)
        {
            sum +=i;
        }
    }
    std::cout << sum;
    return 0;
}
